{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "ES6"
        ],
        "types": [
            "jest","node"
        ],
        "strict": true,
        "moduleResolution": "node", /* Specify module resolution strategy: 'node' (Node.js) or 'classic' (TypeScript pre-1.6). */
        "esModuleInterop": true,
        /* Advanced Options */
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}